<h1>Listing homes</h1>

<table>
  <tr>
    <th>Address</th>
    <th>Apartment</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Country</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @homes.each do |home| %>
  <tr>
    <td><%= home.address %></td>
    <td><%= home.apartment %></td>
    <td><%= home.city %></td>
    <td><%= home.state %></td>
    <td><%= home.zip %></td>
    <td><%= home.country %></td>
    <td><%= home.phone %></td>
    <td><%= link_to 'Show', home %></td>
    <td><%= link_to 'Edit', edit_home_path(home) %></td>
    <td><%= link_to 'Destroy', home, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Home', new_home_path %>
